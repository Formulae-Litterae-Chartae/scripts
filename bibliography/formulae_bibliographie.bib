@book{Fouracre.1986,
 abstract = {Disputes in late fifth- and sixth-century Gaul : some problems / Ian Wood -- 'Placita' and the settlement of disputes in later Merovingian Francia / Paul Fouracre -- Dispute settlement in Carolingian West Francia / Janet L. Nelson -- People and places in dispute in ninth-century Brittany / Wendy Davies -- Visigothic law and regional custom in disputes in early medieval Spain / Roger Collins -- Land disputes and their social framework in Lombard-Carolingian Italy, 700-900 / Chris Wickham -- Dispute settlement in the Byzantine provinces in the tenth century / Rosemary Morris -- Charters, law and the settlement of disputes in Anglo-Saxon England / Patrick Wormald -- Dispute settlement in medieval Ireland : a preliminary inquiry / Richard Sharpe -- An early modern postscript : the Sandlaw dispute, 1546 / Jenny Wormald

This is a collection of original essays on the settlement of disputes in the early middle ages, a subject of central importance for social and political history. Case material, from the evidence of charters, is used to reveal the realities of the settlement process in the behaviour and interactions of people - instead of the prescriptive and idealised models of law-codes and edicts. The book is not therefore a technical study of charters evidence. The geographical range across Europe is unusually wide, which allows comparison across differing societies. Frankish material is inevitably prominent, but the contributors have sought to integrate Celtic, Greek, Italian and Spanish material into the mainstream of the subject. Above all, the book aims to 'demystify' the study of early medieval law, and to present a radical reappraisal of established assumptions about law and society},
 year = {1986},
 title = {The settlement of disputes in early medieval Europe},
 address = {Cambridge},
 publisher = {{Cambridge University Press}},
 isbn = {9780511562310},
 editor = {Fouracre, Paul and Davies, Wendy},
 doi = {10.1017/CBO9780511562310},
 kurztitel = {W. Davies/P. Fouracre, Settlement of disputes}
}


